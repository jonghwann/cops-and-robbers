{
  "expo": {
    "name": "cops and robbers",
    "slug": "cops-and-robbers",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cops-and-robbers",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jangjonghwan.copsandrobbers",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jangjonghwan.copsandrobbers"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8904969-ae7a-4d8f-a149-534e1aa97eca"
      }
    }
  }
}
